D.Size = UDim2.new(0, 50, 0, 50)
D.Font = Enum.Font.Kalam
D.Text = "D"
D.TextColor3 = Color3.fromRGB(170, 170, 255)
D.TextSize = 30.000

UICorner_2.Parent = D

