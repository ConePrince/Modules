local ScreenGui = Instance.new("ScreenGui")
ScreenGui.ResetOnSpawn = false
local W = Instance.new("TextButton")
local D = Instance.new("TextButton")
local S = Instance.new("TextButton")
local A = Instance.new("TextButton")
local Space = Instance.new("TextButton")



local ScreenGui = Instance.new("ScreenGui")
local W = Instance.new("TextButton")
local UICorner = Instance.new("UICorner")
local D = Instance.new("TextButton")
local UICorner_2 = Instance.new("UICorner")
local S = Instance.new("TextButton")
local UICorner_3 = Instance.new("UICorner")
local A = Instance.new("TextButton")
local UICorner_4 = Instance.new("UICorner")
local Space = Instance.new("TextButton")
local UICorner_5 = Instance.new("UICorner")


ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

W.Name = "W"
W.Parent = ScreenGui
W.BackgroundColor3 = Color3.fromRGB(163, 83, 255)
W.BackgroundTransparency = 0.400
W.BorderColor3 = Color3.fromRGB(0, 0, 0)
W.Position = UDim2.new(0.0423289835, 0, 0.801486731, 0)
W.Size = UDim2.new(0, 50, 0, 50)
W.Font = Enum.Font.Kalam
W.Text = "W"
W.TextColor3 = Color3.fromRGB(170, 170, 255)
W.TextSize = 30.000
W.TextWrapped = true

UICorner.Parent = W

D.Name = "D"
D.Parent = ScreenGui
D.BackgroundColor3 = Color3.fromRGB(163, 83, 255)
D.BackgroundTransparency = 0.400
D.BorderColor3 = Color3.fromRGB(0, 0, 0)
