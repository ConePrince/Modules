local ScreenGui = Instance.new("ScreenGui")
local W = Instance.new("TextButton")
local D = Instance.new("TextButton")
local S = Instance.new("TextButton")
local A = Instance.new("TextButton")
local Space = Instance.new("TextButton")


ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

W.Name = "W"
W.Parent = ScreenGui
W.BackgroundColor3 = Color3.fromRGB(170, 170, 255)
W.BorderColor3 = Color3.fromRGB(170, 85, 255)
W.Position = UDim2.new(0.0587033629, 0, 0.766832888, 0)
W.Size = UDim2.new(0, 53, 0, 53)
W.Font = Enum.Font.Unknown
W.Text = "W"
W.TextColor3 = Color3.fromRGB(255, 255, 255)
W.TextSize = 30.000

UICorner.CornerRadius = UDim.new(0, 16)
UICorner.Parent = W

D.Name = "D"
D.Parent = ScreenGui
D.BackgroundColor3 = Color3.fromRGB(170, 170, 255)
D.BorderColor3 = Color3.fromRGB(170, 85, 255)
D.Position = UDim2.new(0.0967271179, 0, 0.842067361, 0)
D.Size = UDim2.new(0, 53, 0, 53)
D.Font = Enum.Font.Unknown
D.Text = "D"
D.TextColor3 = Color3.fromRGB(255, 255, 255)
D.TextSize = 30.000

UICorner_2.CornerRadius = UDim.new(0, 16)
UICorner_2.Parent = D

S.Name = "S"
S.Parent = ScreenGui
S.BackgroundColor3 = Color3.fromRGB(170, 170, 255)
S.BorderColor3 = Color3.fromRGB(170, 85, 255)
S.Position = UDim2.new(0.0587033629, 0, 0.842067361, 0)
S.Size = UDim2.new(0, 53, 0, 53)
