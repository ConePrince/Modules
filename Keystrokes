Space.Name = "Space"
Space.Parent = ScreenGui
Space.BackgroundColor3 = Color3.fromRGB(170, 170, 255)
Space.BorderColor3 = Color3.fromRGB(170, 85, 255)
Space.Position = UDim2.new(0.0200125165, 0, 0.918645382, 0)
Space.Size = UDim2.new(0, 176, 0, 31)
Space.Font = Enum.Font.Unknown
Space.Text = "________"
Space.TextColor3 = Color3.fromRGB(255, 255, 255)
Space.TextSize = 30.000

UICorner_5.CornerRadius = UDim.new(0, 16)



local UserInputService = game:GetService("UserInputService")


local wButton = ScreenGui.W


local litColor = Color3.fromRGB(220,220,220)


local unlitColor = Color3.fromRGB(255, 255, 255)


UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if not gameProcessed then
        
        if input.KeyCode == Enum.KeyCode.W then
            
            wButton.BackgroundColor3 = litColor
        end
    end
end)


UserInputService.InputEnded:Connect(function(input, gameProcessed)
    if not gameProcessed then
        
        if input.KeyCode == Enum.KeyCode.W then
            
            wButton.BackgroundColor3 = unlitColor
        end
    end
end)


